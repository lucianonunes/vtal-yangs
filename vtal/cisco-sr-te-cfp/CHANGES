   A. Removed head-end actions for SR-Policy and other odn-template restrictions

    ```
    augment /cisco-sr-te-cfp:sr-te {
        container policies {
          tailf:info "SR-TE Policy container";
          description "SR-TE Policy container";
          list policy {
            tailf:info "SR-Policy";
            description "SR-Policy";
            key "name";
            uses ncs:service-data;
            ncs:servicepoint sr-policies-external-servicepoint;
            uses ct-hook:template-hook;
            leaf name {
              tailf:info "Unique identifier for a SR-Policy";
              description "Unique identifier for a SR-Policy";
              type cisco-sr-te-cfp-sr-types:Cisco-ios-xr-string;
            }
            list head-end {
              tailf:info "List of headend routers";
              description "List of headend routers";
              key name;
              leaf name {
                type leafref {
                  path "/ncs:devices/ncs:device/ncs:name";
                }
              }
              min-elements 1;
              uses ct-hook:template-hook; ------------------------------------------------------------> REMOVED
              leaf head-end-redeploy-flag { ------------------------------------------------------------> REMOVED FLAG
                tailf:hidden tsdn;
                tailf:info "Custom redeploy flag that redeploys lower level services if changed.";
                description "Custom redeploy flag that redeploys lower level services if changed.";
                type string;
              }
              container action {  -------------------------------------------------------------------------> REMOVED ALL ACTIONS PER HEAD-END
                tailf:action self-test {
                  tailf:actionpoint sr-policy-external-self-test-actionpoint;
                  input {
                  }
                  output {
                    leaf status {
                      type string;
                    }
                    leaf message {
                      type string;
                    }
                  }
                }
                tailf:action redeploy {
                  tailf:actionpoint sr-policy-head-end-redeploy-actionpoint;
                  input {
                  }
                  output {
                    leaf status {
                      type string;
                    }
                    leaf message {
                      type string;
                    }
                  }
                }
              }
            }

            must "(not(odn-template) and count(head-end)=1) or (odn-template)" { ------------------> REMOVED MUST CHECK - MAX 1 HEAD-END ALLOWED
              error-message "Only one head-end is allowed for non-ODN SR Policy.";
            }

            container odn-template {  -------------------------------------------------------------> REMOVED ENTIRE CONTAINER
              tailf:info "Designates this policy as a ODN Template";
              description "Designates this policy as a ODN Template";
              presence "true";
              leaf maximum-sid-depth {
                  tailf:info "Maximum SID Depth";
                  description "Maximum SID Depth";
                  type uint8 {
                      tailf:info "Maximum SID Depth <1-255>";
                      range "1..255";
                  }
              }
            }
            leaf tail-end {
              tailf:info "Router-Id of the tail-end device of the SR policy";
              description "Router-Id of the tail-end device of the SR policy";
              type union {
                type string;
                type inet:ipv4-address;
                type inet:ipv6-address;
              }
              mandatory true;
              when "not(boolean(../odn-template))"; ------------------------------------------------> REMOVED MUST CHECK
            }
            ...
            leaf binding-sid {
              tailf:info "Binding SID of the SR-Policy applied at the headend";
              description "Binding SID of the SR-Policy applied at the headend";
              type uint32;
              when "not(boolean(../odn-template))"; -------------------------------------------------> REMOVED MUST CHECK
            }
          ...
             list path {
              tailf:info "SR-Policy candidate paths";
              description "SR-Policy candidate paths";
              key preference;
              ...
              min-elements 1;
              choice sr-te-path-choice {
                container explicit {
                  presence "true";
                  ...
                  when "not(boolean(../../odn-template))";  --------------------------------------------> REMOVED MUST CHECK
                }
                container dynamic {
                  presence "true";
                  tailf:info "Use dynamic path computation";
                  description "Use dynamic path computation";
                  ...
                  leaf flex-alg { ----------------------------------------------------------------------> REMOVED LEAF
                    type uint32 {
                      range "128..255";
                    }
                    tailf:info "Prefix-SID algorithm";
                    description "Prefix-SID algorithm";
                    when "boolean(../../../odn-template)";
                  }
                  ...
                  container constraints {
                    leaf sid-limit {
                      tailf:info "Absolute limit on the number of SIDs in the SID-list";
                      description "Absolute limit on the number of SIDs in the SID-list";
                      type uint32 {
                        range "0..255";
                      }
                      when "not(boolean(../../../../odn-template))"; -------------------------------------> REMOVED MUST CHECK
                    }
                    uses constraints-grp;
                  }
                }
              }
            }
            must "(boolean(odn-template) and count(path) = 1) or (not(boolean(odn-template)))" {----------------> REMOVED MUST CHECK
              error-message "With ODN, only one path definition allowed";
            }
    ...
            container auto-route {
              tailf:info "Enable Autoroute on this SR-policy";
              description "Enable Autoroute on this SR-policy";
              presence true;
              ...
              when "not(boolean(../odn-template))"; --------------------------------------------------------------> REMOVED MUST CHECK
            }


    ...
          container action {
              ...
              tailf:action get-modifications {
                tailf:actionpoint sr-policy-get-modifications-actionpoint;
                input {
                  leaf device { ----------------------------------------------------------------------------------> REMOVED DEVICE INPUT due to 1 HEAD-END
                    type leafref {
                      path "../../../head-end/name";
                    }
                  }
                  leaf outformat {
                    type outformat2;
                  }
                }
                output {
                  leaf message {
                    type string;
                  }
                }
              }
            }
