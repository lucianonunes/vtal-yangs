     binding-sid mpls 111
     color 111 end-point ipv4 1.1.1.1
     autoroute
     !
     candidate-paths
      preference 111
       dynamic
        metric
         sid-limit 10
         type te
        !
       !
       constraints
        segments
         sid-algorithm 128
        !
        affinity
         include-all
          name RED
          name GREEN
         !
        !
        disjoint-path group-id 10 type node -------------> This is configured now for non-ODN service.
       !
      !
     !
