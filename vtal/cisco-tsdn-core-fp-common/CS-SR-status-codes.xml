<config xmlns="http://tail-f.com/ns/config/1.0">
  <status-code-cfp xmlns="http://com/cisco/cfp-common-status-codes">
    <name>CS-SR</name>
  </status-code-cfp>
  <status-codes xmlns="http://com/cisco/cfp-common-status-codes">
    <core-function-pack>
      <name>CS-SR</name>
      <status-code-enum-path>cisco-tsdn-core-fp-common/python/cisco_tsdn_core_fp_common/status_codes/cs_sr_te_status_codes</status-code-enum-path>
      <status-code>
        <code>701</code>
        <reason>Allocation has failed</reason>
        <category>resource</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>702</code>
        <reason>Allocation request failed</reason>
        <category>resource</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>503</code>
        <reason>Cleanup failed</reason>
        <category>custom-action</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>870</code>
        <reason>Custom Template apply failed</reason>
        <category>custom-action</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>801</code>
        <reason>Service generated name conflicts with existing config</reason>
        <category>service</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>407</code>
        <reason>Missing input</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that missing input elements are provided in the payload</recommended-actions>
      </status-code>
      <status-code>
        <code>504</code>
        <reason>Service Recovery failed</reason>
        <category>custom-action</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>400</code>
        <reason>Status code mapping has not been loaded for function pack during install</reason>
        <category>user</category>
        <severity>ERROR</severity>
        <recommended-actions>Bootstrap status code mapping</recommended-actions>
      </status-code>
    </core-function-pack>
  </status-codes>
</config>
