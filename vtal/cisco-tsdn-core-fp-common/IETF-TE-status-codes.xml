<config xmlns="http://tail-f.com/ns/config/1.0">
  <status-code-cfp xmlns="http://com/cisco/cfp-common-status-codes">
    <name>IETF-TE</name>
  </status-code-cfp>
  <status-codes xmlns="http://com/cisco/cfp-common-status-codes">
    <core-function-pack>
      <name>IETF-TE</name>
      <status-code-enum-path>cisco-tsdn-core-fp-common/python/cisco_tsdn_core_fp_common/status_codes/ietf_te_status_codes</status-code-enum-path>
      <status-code>
        <code>503</code>
        <reason>Cleanup failed</reason>
        <category>custom-action</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>303</code>
        <reason>Config push failed</reason>
        <category>device</category>
        <severity>ERROR</severity>
        <recommended-actions>Device configuration rejected, fix the service payload and perform recovery steps.</recommended-actions>
      </status-code>
      <status-code>
        <code>301</code>
        <reason>Device unreachable</reason>
        <category>device</category>
        <severity>ERROR</severity>
        <recommended-actions>Check device connectivity from NSO and perform recovery steps.</recommended-actions>
      </status-code>
      <status-code>
        <code>302</code>
        <reason>Device out of sync</reason>
        <category>device</category>
        <severity>ERROR</severity>
        <recommended-actions>Check sync between device and NSO, and perform recovery steps.</recommended-actions>
      </status-code>
      <status-code>
        <code>403</code>
        <reason>Input element is not supported</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that input element is supported in the payload</recommended-actions>
      </status-code>
      <status-code>
        <code>402</code>
        <reason>Required element is missing</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that required element is included in the payload</recommended-actions>
      </status-code>
      <status-code>
        <code>407</code>
        <reason>Missing input</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that missing input elements are provided in the payload</recommended-actions>
      </status-code>
      <status-code>
        <code>504</code>
        <reason>Service Recovery failed</reason>
        <category>custom-action</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>501</code>
        <reason>Self test failed</reason>
        <category>custom-action</category>
        <severity>ERROR</severity>
      </status-code>
      <status-code>
        <code>400</code>
        <reason>Status code mapping has not been loaded for function pack during install</reason>
        <category>user</category>
        <severity>ERROR</severity>
        <recommended-actions>Bootstrap status code mapping</recommended-actions>
      </status-code>
      <status-code>
        <code>406</code>
        <reason>Input element's value is not a valid format</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that input element's value is a valid format</recommended-actions>
      </status-code>
      <status-code>
        <code>408</code>
        <reason>Input element's value is not consistent</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that input element's value is consistent in the payload</recommended-actions>
      </status-code>
      <status-code>
        <code>404</code>
        <reason>The value for the input element is not supported</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that input element's value is supported in the payload</recommended-actions>
      </status-code>
      <status-code>
        <code>405</code>
        <reason>Input element's value is out of range</reason>
        <category>validation</category>
        <severity>ERROR</severity>
        <recommended-actions>Verify that input element's value is in range in the payload</recommended-actions>
      </status-code>
    </core-function-pack>
  </status-codes>
</config>
